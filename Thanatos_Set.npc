function	script	Valkyrie_Effect	{
	if(!getarg(0)) {
		debugmes "Valkyrie_Effect: No se incluyo una ID para el efecto del item.";
		end;
	}
	.@r = getrefine();
	switch (getarg(0)) {
		// Error
		case 0:
		default:
			debugmes "No posee script item ("+getarg(0)+"): Valkyrie_Effect ";
			end;
	}
	return;
}
