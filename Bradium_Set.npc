function	script	Bradium_Effect	{
	if(!getarg(0)) {
		debugmes "Bradium_Effect: No se incluyo una ID para el efecto del item.";
		end;
	}
	.@r = getrefine();
	switch (getarg(0)) {
		// Error
		case 0:
		default:
			debugmes "No posee script item ("+getarg(0)+"): Bradium_Effect ";
			end;
	}
	return;
}
